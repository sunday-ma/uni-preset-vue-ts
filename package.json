    "mp-html": "^2.5.0",
    "nutui-uniapp": "^1.7.11",
    "pinia": "2.1.7",
    "vue": "^3.4.21",
    "vue-i18n": "^9.1.9",
    "z-paging": "^2.7.11"
  },
  "devDependencies": {
    "@alova/scene-vue": "^1.6.1",
    "@antfu/eslint-config": "^2.21.1",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@dcloudio/types": "^3.4.8",
    "@dcloudio/uni-automator": "3.0.0-4010520240507001",

{
  "name": "uni-preset-vue",
  "version": "0.0.4",
  "description": "uni-preset-vue",
  "engines": {
    "node": ">=18",
    "pnpm": ">=7.30"
  },
  "scripts": {
    "dev": "uni",
    "dev:app": "uni -p app",
    "dev:app-android": "uni -p app-android",
    "dev:app-ios": "uni -p app-ios",
    "dev:app-harmony": "uni -p app-harmony",
    "dev:custom": "uni -p",
    "dev:h5": "uni",
    "dev:h5:ssr": "uni --ssr",
    "dev:mp-alipay": "uni -p mp-alipay",
    "dev:mp-baidu": "uni -p mp-baidu",
    "dev:mp-jd": "uni -p mp-jd",
    "dev:mp-kuaishou": "uni -p mp-kuaishou",
    "dev:mp-lark": "uni -p mp-lark",
    "dev:mp-qq": "uni -p mp-qq",
    "dev:mp-toutiao": "uni -p mp-toutiao",
    "dev:mp-weixin": "uni -p mp-weixin",
    "dev:mp-xhs": "uni -p mp-xhs",
    "dev:quickapp-webview": "uni -p quickapp-webview",
    "dev:quickapp-webview-huawei": "uni -p quickapp-webview-huawei",
    "dev:quickapp-webview-union": "uni -p quickapp-webview-union",
    "build:app": "uni build -p app",
    "build:app-android": "uni build -p app-android",
    "build:app-ios": "uni build -p app-ios",
    "build:app-harmony": "uni build -p app-harmony",
    "build:custom": "uni build -p",
    "build:h5": "uni build",
    "build:h5:ssr": "uni build --ssr",
    "build:mp-alipay": "uni build -p mp-alipay",
    "build:mp-baidu": "uni build -p mp-baidu",
    "build:mp-jd": "uni build -p mp-jd",
    "build:mp-kuaishou": "uni build -p mp-kuaishou",
    "build:mp-lark": "uni build -p mp-lark",
    "build:mp-qq": "uni build -p mp-qq",
    "build:mp-toutiao": "uni build -p mp-toutiao",
    "build:mp-weixin": "uni build -p mp-weixin",
    "build:mp-weixin-ci": "uni build -p mp-weixin && ci-mp-wx",
    "build:mp-xhs": "uni build -p mp-xhs",
    "build:quickapp-webview": "uni build -p quickapp-webview",
    "build:quickapp-webview-huawei": "uni build -p quickapp-webview-huawei",
    "build:quickapp-webview-union": "uni build -p quickapp-webview-union",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "release": "release-it"
  },
  "dependencies": {
    "@alova/adapter-uniapp": "^2.0.6",
    "@dcloudio/uni-app": "3.0.0-4020420240722002",
    "@dcloudio/uni-app-harmony": "3.0.0-4020420240722002",
    "@dcloudio/uni-app-plus": "3.0.0-4020420240722002",
    "@dcloudio/uni-components": "3.0.0-4020420240722002",
    "@dcloudio/uni-h5": "3.0.0-4020420240722002",
    "@dcloudio/uni-mp-alipay": "3.0.0-4020420240722002",
    "@dcloudio/uni-mp-baidu": "3.0.0-4020420240722002",
    "@dcloudio/uni-mp-jd": "3.0.0-4020420240722002",
    "@dcloudio/uni-mp-kuaishou": "3.0.0-4020420240722002",
    "@dcloudio/uni-mp-lark": "3.0.0-4020420240722002",
    "@dcloudio/uni-mp-qq": "3.0.0-4020420240722002",
    "@dcloudio/uni-mp-toutiao": "3.0.0-4020420240722002",
    "@dcloudio/uni-mp-weixin": "3.0.0-4020420240722002",
    "@dcloudio/uni-mp-xhs": "3.0.0-4020420240722002",
    "@dcloudio/uni-quickapp-webview": "3.0.0-4020420240722002",
    "@uni-helper/uni-env": "^0.1.4",
    "@vueuse/core": "^11.0.3",
    "@zxlee/z-tabs": "^0.2.7",
    "airpower": "^1.1.0",
    "alova": "^3.0.16",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "lodash": "^4.17.21",
    "mp-html": "^2.5.0",
    "nutui-uniapp": "^1.7.17",
    "pinia": "2.2.2",
    "vue": "3.4.21",
    "vue-i18n": "^9.14.0",
    "z-paging": "^2.7.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.6.2",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@dcloudio/types": "^3.4.12",
    "@dcloudio/uni-automator": "3.0.0-4020420240722002",
    "@dcloudio/uni-cli-shared": "3.0.0-4020420240722002",
    "@dcloudio/uni-stacktracey": "3.0.0-4020420240722002",
    "@dcloudio/vite-plugin-uni": "3.0.0-4020420240722002",
    "@iconify-json/carbon": "^1.2.1",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.16.5",
    "@uni-helper/eslint-config": "^0.2.0",
    "@uni-helper/uni-types": "1.0.0-alpha.3",
    "@uni-helper/vite-plugin-uni-components": "^0.1.0",
    "@uni-helper/vite-plugin-uni-layouts": "^0.1.10",
    "@uni-helper/vite-plugin-uni-manifest": "^0.2.6",
    "@uni-helper/vite-plugin-uni-pages": "^0.2.27",
    "@unocss/eslint-plugin": "^0.62.4",
    "@unocss/transformer-directives": "^0.62.4",
    "@unocss/transformer-variant-group": "^0.62.4",
    "@vue/runtime-core": "^3.4.38",
    "@vue/tsconfig": "^0.5.1",
    "ci-mp-wx": "^1.0.1",
    "eslint": "^9.10.0",
    "eslint-plugin-format": "^0.1.2",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "miniprogram-api-typings": "^3.12.3",
    "release-it": "^17.6.0",
    "sass": "^1.78.0",
    "typescript": "~5.6.2",
    "unocss": "^0.62.4",
    "unocss-preset-weapp": "^0.62.2",
    "unplugin-auto-import": "^0.18.3",
    "validate-branch-name": "^1.3.1",
    "vite": "5.4.6",
    "vue-tsc": "^2.0.28"
  },
  "lint-staged": {
    "**/*.{vue,js,ts,jsx,tsx}": [
      "eslint . --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "validate-branch-name": {
    "pattern": "^(master|main|develop){1}$|^(build|chore|ci|docs|feat|fix|perf|refactor|revert|style|test|wip|refine)/.+$",
    "errorMsg": "分支命名不规范，请修正!"
  }
}

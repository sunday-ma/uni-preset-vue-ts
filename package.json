    "@types/wechat-miniprogram": "^3.4.7",
    "@uni-helper/uni-app-types": "^0.5.13",
    "@uni-helper/vite-plugin-uni-components": "^0.0.9",
    "@uni-helper/vite-plugin-uni-layouts": "^0.1.10",
    "@uni-helper/vite-plugin-uni-manifest": "^0.2.6",
    "@uni-helper/vite-plugin-uni-pages": "^0.2.25",
    "@unocss/eslint-plugin": "^0.61.3",
    "@unocss/transformer-directives": "^0.61.0",
    "@unocss/transformer-variant-group": "^0.61.0",
    "@vue/runtime-core": "^3.4.21",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^9.5.0",
    "eslint-plugin-format": "^0.1.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "release-it": "^17.2.1",
    "sass": "^1.77.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "unocss": "^0.61.0",
    "unocss-preset-weapp": "^0.61.0",
    "unplugin-auto-import": "^0.17.6",
    "vite": "5.2.8",
    "vue-tsc": "^1.0.24"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "9"
      }
    }
  },
  "lint-staged": {
    "**/*.{vue,js,ts,jsx,tsx}": [
      "eslint . --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
